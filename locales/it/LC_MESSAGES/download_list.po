#: Plugin.__init__:...
msgid "🔄 Loading Download by List plugin..."
msgstr "🔄 Caricamento del plugin Download by List in corso..."

msgid "🔔 Plugin loaded."
msgstr "🔔 Plugin caricato."

msgid "🔧 Creating main window..."
msgstr "🔧 Creazione della finestra principale..."

msgid "Download by List"
msgstr "Download per Lista"

msgid "Enter search terms below:"
msgstr "Inserisci i termini di ricerca qui sotto:"

msgid "Audio Format:"
msgstr "Formato audio:"

msgid "Audio Quality:"
msgstr "Qualità audio:"

msgid "Search and Download"
msgstr "Cerca e scarica"

msgid "No valid content in the text area."
msgstr "Nessun contenuto valido nell'area di testo."

msgid "⚠️ Please enter at least one search term."
msgstr "⚠️ Inserisci almeno un termine di ricerca."

msgid "⚠️ No valid search terms found."
msgstr "⚠️ Nessun termine di ricerca valido trovato."

msgid "🔍 Starting searches for {count} term(s)"
msgstr "🔍 Avvio ricerche per {count} termine(i)"

msgid "📡 Searching for: {term}"
msgstr "📡 Ricerca per: {term}"

msgid "❌ Error during search for '{term}': {error}"
msgstr "❌ Errore durante la ricerca per '{term}': {error}"

msgid "❌ No file found for:\n "
msgstr "❌ Nessun file trovato per:\n "

msgid "✅ All files have been found."
msgstr "✅ Tutti i file sono stati trovati."

msgid "❌ Window closed."
msgstr "❌ Finestra chiusa."

msgid "🖱️ 'Search and Download' button clicked!"
msgstr "🖱️ Pulsante 'Cerca e scarica' premuto!"

msgid "📩 Received search results..."
msgstr "📩 Risultati della ricerca ricevuti..."

msgid "❌ Error extracting tuple: {error}"
msgstr "❌ Errore nell'estrazione della tupla: {error}"

msgid "❌ Error converting bitrate: {error}"
msgstr "❌ Errore nella conversione del bitrate: {error}"

msgid "🎯 Checking file: {filename} (Format: {h_format}, Quality: {h_quality}, Format match: {format_match}, Quality match: {quality_match}, private: {is_private})"
msgstr "🎯 Verifica del file: {filename} (Formato: {h_format}, Qualità: {h_quality}, Formato corrisponde: {format_match}, Qualità corrisponde: {quality_match}, privato: {is_private})"

msgid "✅ Matching result found for {term}: {filename}"
msgstr "✅ Risultato corrispondente trovato per {term}: {filename}"

msgid "❌ No matching file found for {term}"
msgstr "❌ Nessun file corrispondente trovato per {term}"

msgid "🚀 Download launched for: {file}"
msgstr "🚀 Download avviato per: {file}"

msgid "❌ Error downloading {file}: {error}"
msgstr "❌ Errore durante il download di {file}: {error}"


